ion.pathname === \"/register\" ||\n  //     (auth?.user && location.pathname !== \"/\")\n  //   )\n  //     setVisibility(false);\n  //   else setVisibility(true);\n  // }, [location, auth.user]);\n\n  return (\n    <DrawerNavigationLayout>\n      <div className={`w-full sticky top-0 ${visibility ? \"block\" : \"hidden\"}`}>\n        <DrawerNavigation>\n          <div className=\"logo-box flex justify-center w-full mt-2\">\n            <Link to=\"/\">\n              <Logo />\n            </Link>\n          </div>\n          <div className=\"mt-8 flex flex-col items-center\">\n            {auth.user && (\n              <DrawerNavigationItem to={`/${auth.user.id}`}>\n                Go to Dashboard\n              </DrawerNavigationItem>\n            )}\n            <DrawerNavigationItem to=\"/\">Home</DrawerNavigationItem>\n            <DrawerNavigationItem to=\"/about#howItWorks\">\n              How It Works?\n            </DrawerNavigationItem>\n            <DrawerNavigationItem to=\"/support\">Support</DrawerNavigationItem>\n            <SignInButton visibility={!auth.user} width=\"w-11/12\" />\n          </div>\n        </DrawerNavigation>\n        <Navbar\n          position=\"relative\"\n          className=\"pl-3 pr-3 sm:pl-8 sm:pr-8 lg:pl-20 lg:pr-20\"\n        >\n          <Link to=\"/\">\n            <Logo />\n          </Link>\n          <div className=\"flex items-center h-full text-sm\">\n            <div className=\"hidden h-full lg:flex\">\n              <NavItem to=\"/\" title=\"Home\" />\n              <NavItem to=\"/about#howItWorks\" title=\"How It Works?\" />\n              <NavItem to=\"/pricing\" title=\"Pricing\" />\n              {auth.user && (\n                <NavItem to={`/${auth.user.id}`}>Go to Dashboard</NavItem>\n              )}\n              <SignInButton visibility={!auth.user} />\n            </div>\n            <div className=\"block lg:hidden\">\n              <DrawerNavigationHandler />\n            </div>\n          </div>\n        </Navbar>\n      </div>\n    </DrawerNavigationLayout>\n  );\n};\n\nconst SignInButton = ({ visibility, width = \"w-max\" }) => {\n  if (visibility)\n    return (\n      <div className={`flex items-center justify-center ${width}`}>\n        <Link to=\"/login\">\n          <Button className=\"small\">Sign In</Button>\n        </Link>\n      </div>\n    );\n  else return <></>;\n};\n\nexport default function MainHeaderRender() {\n  const location = useLocation();\n\n  if (\n    location.pathname === \"/login\" ||\n    location.pathname === \"/register\" ||\n    location.pathname !== \"/\"\n  )\n    return <></>;\n  else return <MainHeader />;\n}\n","/mnt/c/Users/kkana/Lab/web50/expense_tracker/frontend/src/components/common/DrawerNavigation.js",[],"/mnt/c/Users/kkana/Lab/web50/expense_tracker/frontend/src/context/ApplicationContext.js",[],"/mnt/c/Users/kkana/Lab/web50/expense_tracker/frontend/src/context/reducers/index.js",[],"/mnt/c/Users/kkana/Lab/web50/expense_tracker/frontend/src/context/actions/index.js",[],"/mnt/c/Users/kkana/Lab/web50/expense_tracker/frontend/src/context/AuthenticationContext.js",[],"/mnt/c/Users/kkana/Lab/web50/expense_tracker/frontend/src/components/icons/BankIcons.js",[],"/mnt/c/Users/kkana/Lab/web50/expense_tracker/frontend/src/components/common/PricingCard.js",[],"/mnt/c/Users/kkana/Lab/web50/expense_tracker/frontend/src/components/common/Section.js",[],"/mnt/c/Users/kkana/Lab/web50/expense_tracker/frontend/src/components/common/Container.js",[],"/mnt/c/Users/kkana/Lab/web50/expense_tracker/frontend/src/components/Footer.js",[],"/mnt/c/Users/kkana/Lab/web50/expense_tracker/frontend/src/hooks/useLoader.js",[],"/mnt/c/Users/kkana/Lab/web50/expense_tracker/frontend/src/hooks/useErrors.js",[],{"ruleId":"248","replacedBy":"249"},{"ruleId":"250","replacedBy":"251"},{"ruleId":"252","severity":1,"message":"253","line":16,"column":6,"nodeType":"254","endLine":16,"endColumn":17,"suggestions":"255"},{"ruleId":"256","severity":1,"message":"257",